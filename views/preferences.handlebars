{{#if user}}
	<div id="username">
		<h1>{{username}}</h1>
		<form action= "/user/username/preferences" method="post">
			New username:
			<br>
			<input type="text" name="username">
			<br>
			<input type="submit" value="Submit">
		</form>
	</div>
	<div id="email">
		<h2>Email: {{email}}</h1>
	</div>

	<div id="timeCreated">
		<h2>Member since: {{timeCreated}}</h1>
	</div>
{{/if}}
